  "dependencies": {
    "@sentry/react": "^6.5.1",
    "antd": "^4.16.0",
    "dayjs": "^1.10.4",
    "framer-motion": "^4.1.17",
    "next": "^11.0.1",
    "phoenix": "^1.5.9",
    "rc-upload": "^4.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "remark-breaks": "^2.0.2",
    "remark-emoji": "^2.2.0",
    "superagent": "^6.1.0",
    "theme-ui": "^0.10.0",
    "tinycolor2": "^1.4.2",
    "use-composed-ref": "^1.1.0",
    "use-latest": "^1.2.0"
