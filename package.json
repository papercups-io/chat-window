  "version": "0.0.1",
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "start": "next start -p 8080",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
